<!DOCTYPE html>
<html>
  <head>
    <title>スタック・オーバーフロー</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <body>
    <div class="top-bar">
      <div class="top-nav">
        <div class="nav_content">
          <h1 id="site-title"><a href="/">stack<span>overflow</span></a></h1>
          <nav class="network-items">
            <li id="question-tab"><a href="/questions/">質問</a></li>
            <li><a href="/tags/">タグ</a></li>
            <li id="users-tab"><a href="/users/">ユーザー</a></li>
          </nav>
          <form id="search" action="#" method="get" autocomplete="off" role="search">
            <input name="q" type="text" placeholder="検索..." value="" tabindex="1" autocomplete="off" maxlength="240">
          </form>
        </div>
        <div class="right-col">
          <% if user_signed_in? %>
          <!--<li><%= link_to 'マイページ', edit_user_registration_path %></li>
          <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete %></li>-->
          <a href="/users/edit" class="mypage">マイページ</a>
          <a href="/users/sign_out" class="logout", data-method="delete">ログアウト</a>
          <% else %>
          <!--<li><%= link_to 'ログイン', new_user_session_path  %></li>
          <li><%= link_to 'サインアップ', new_user_registration_path %></li>-->
          <a href="/users/sign_in" class="login">ログイン</a>
          <a href="/users/sign_up" class="button">登録</a>
          <% end %>
        </div>
      </div>
    </div>
    <div class="l-grid main-content">
      <% if flash[:notice] %>
        <div class="alert alert-success text-center">
          <%= notice %>
        </div>
      <% end %>
      <%= yield %>
    </div>
